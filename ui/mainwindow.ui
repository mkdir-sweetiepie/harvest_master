   <string notr="true">/* Main Window Styling */
QMainWindow {
    background-color: #0a0e1a;
    color: #e0e6ed;
    font-family: 'Segoe UI', Arial, sans-serif;
}

/* Header */
QFrame#headerFrame {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, 
                stop:0 #1a4d2e, stop:1 #27ae60);
    border-radius: 12px;
    margin: 5px;
}

QLabel#titleLabel {
    color: white;
    font-size: 28px;
    font-weight: bold;
    padding: 20px;
}

QLabel#statusLabel {
    color: #2ecc71;
    font-size: 16px;
    font-weight: bold;
    padding: 10px 20px;
}

/* Control Cards */
QGroupBox {
    background-color: #1e2328;
    border: 2px solid #3c4043;
    border-radius: 12px;
    font-size: 16px;
    font-weight: bold;
    color: #e0e6ed;
    margin: 8px;
    padding-top: 20px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 20px;
    padding: 0 10px 0 10px;
    color: #4a90e2;
    font-size: 18px;
}

/* Buttons */
QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #4a90e2, stop:1 #357abd);
    border: none;
    border-radius: 10px;
    color: white;
    font-size: 14px;
    font-weight: bold;
    padding: 15px 25px;
    margin: 5px;
}

QPushButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #5ba0f2, stop:1 #458acd);
}

QPushButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #3680d2, stop:1 #2570ad);
}

QPushButton#emergencyBtn {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #e74c3c, stop:1 #c0392b);
    font-size: 16px;
    padding: 20px;
}

QPushButton#emergencyBtn:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #f75c4c, stop:1 #d0493b);
}

QPushButton#harvestBtn {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #27ae60, stop:1 #229954);
    font-size: 16px;
    padding: 20px;
}

QPushButton#harvestBtn:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                stop:0 #37be70, stop:1 #32a964);
}

/* Progress Bars */
QProgressBar {
    border: 2px solid #3c4043;
    border-radius: 10px;
    background-color: #2c3e50;
    text-align: center;
    color: white;
    font-weight: bold;
    font-size: 14px;
    height: 25px;
}

QProgressBar::chunk {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                stop:0 #27ae60, stop:1 #2ecc71);
    border-radius: 8px;
}

/* Labels */
QLabel {
    color: #e0e6ed;
    font-size: 14px;
}

QLabel#valueLabel {
    color: #2ecc71;
    font-size: 20px;
    font-weight: bold;
}

QLabel#warningLabel {
    color: #f39c12;
    font-size: 16px;
    font-weight: bold;
}

QLabel#errorLabel {
    color: #e74c3c;
    font-size: 16px;
    font-weight: bold;
}

/* Slider */
QSlider::groove:horizontal {
    border: 2px solid #3c4043;
    height: 12px;
    background: #2c3e50;
    border-radius: 6px;
}

QSlider::handle:horizontal {
    background: #4a90e2;
    border: 2px solid #357abd;
    width: 24px;
    margin: -6px 0;
    border-radius: 12px;
}

QSlider::sub-page:horizontal {
    background: #27ae60;
    border-radius: 6px;
}

/* Text Edit for logs */
QTextEdit {
    background-color: #2c3e50;
    border: 2px solid #3c4043;
    border-radius: 8px;
    color: #e0e6ed;
    font-family: 'Consolas', monospace;
    font-size: 12px;
    padding: 10px;
}</string>
